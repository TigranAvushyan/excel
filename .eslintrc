{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-debugger": "off",
    "curly": "off",
    "valid-jsdoc": "off",
    "one-var": "off",
    "no-case-declarations": "off"
  },
  "extends": ["eslint:recommended", "google"]
}